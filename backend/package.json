{
  "name": "@music-monitor/backend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "tsx watch --env-file=.env src/index.ts",
    "build": "prisma generate && tsc",
    "postinstall": "prisma generate",
    "start": "node dist/index.js",
    "start:with-migrate": "pnpm run db:migrate:deploy && pnpm run db:generate && node dist/index.js",
    "lint": "eslint src --ext .ts",
    "test": "vitest",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:push": "prisma db push",
    "db:seed": "tsx src/db/seed.ts"
  },
  "dependencies": {
    "@fastify/cors": "^9.0.1",
    "@fastify/env": "^4.3.0",
    "@fastify/jwt": "^8.0.0",
    "@fastify/multipart": "^8.3.1",
    "@fastify/rate-limit": "^9.1.0",
    "@prisma/client": "^5.9.1",
    "@sendgrid/mail": "^8.1.6",
    "@supabase/supabase-js": "^2.55.0",
    "apify-client": "^2.15.0",
    "axios": "^1.6.7",
    "bandcamp-fetch": "^3.0.0",
    "date-fns": "^3.3.1",
    "fastify": "^4.26.0",
    "googleapis": "^155.0.1",
    "node-cron": "^3.0.3",
    "p-queue": "^6.6.2",
    "pino": "^8.18.0",
    "pino-pretty": "^10.3.1",
    "playwright": "^1.54.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@types/node-cron": "^3.0.11",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@typescript-eslint/parser": "^6.20.0",
    "eslint": "^8.56.0",
    "prisma": "^5.9.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^1.2.2"
  }
}